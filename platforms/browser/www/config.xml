<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.intra" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>ACD News</name>
    <description>
        ACD News
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        ACD Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <splash platform="android" qualifier="port-ldpi" src="res/screen/android/screen-hdpi-portrait.png" />
    <splash platform="android" qualifier="port-mdpi" src="res/screen/android/screen-hdpi-portrait.png" />
    <splash platform="android" qualifier="port-hdpi" src="res/screen/android/screen-hdpi-portrait.png" />
    <splash platform="android" qualifier="port-xhdpi" src="res/screen/android/drawable-port-xhdpi-screen.png" />
    <splash platform="android" qualifier="port-xxhdpi" src="res/screen/android/drawable-port-xxhdpi-screen.png" />
    <splash platform="android" qualifier="port-xxxhdpi" src="res/screen/android/drawable-port-xxxhdpi-screen.png" />
    <splash platform="android" qualifier="land-ldpi" src="res/screen/android/screen-hdpi-landscape.png" />
    <splash platform="android" qualifier="land-mdpi" src="res/screen/android/screen-hdpi-landscape.png" />
    <splash platform="android" qualifier="land-hdpi" src="res/screen/android/screen-hdpi-landscape.png" />
    <splash platform="android" qualifier="land-xhdpi" src="res/screen/android/drawable-port-xhdpi-screen.png" />
    <splash platform="android" qualifier="land-xxhdpi" src="res/screen/android/drawable-port-xxhdpi-screen.png" />
    <splash platform="android" qualifier="land-xxxhdpi" src="res/screen/android/drawable-port-xxxhdpi-screen.png" />
    <splash height="1136" platform="ios" src="www/res/screen/ios/screen-iphone-568h-2x.png" width="640" />
    <splash height="1334" platform="ios" src="www/res/screen/ios/screen-iphone-portrait-667h.png" width="750" />
    <splash height="2208" platform="ios" src="www/res/screen/ios/screen-iphone-landscape-736h.png" width="1242" />
    <splash height="1242" platform="ios" src="www/res/screen/ios/screen-iphone-landscape-736h.png" width="2208" />
    <splash height="1536" platform="ios" src="www/res/screen/ios/screen-ipad-landscape-2x.png" width="2048" />
    <splash height="768" platform="ios" src="www/res/screen/ios/screen-ipad-landscape.png" width="1024" />
    <splash height="2048" platform="ios" src="www/res/screen/ios/screen-ipad-portrait-2x.png" width="1536" />
    <splash height="1024" platform="ios" src="www/res/screen/ios/screen-ipad-portrait-2x.png" width="768" />
    <splash height="960" platform="ios" src="www/res/screen/ios/screen-iphone-portrait-2x.png" width="640" />
    <splash height="480" platform="ios" src="www/res/screen/ios/screen-iphone-portrait.png" width="320" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="SplashScreen" value="splash" />
    <preference name="StatusBarOverlaysWebView" value="true" />
    <preference name="StatusBarBackgroundColor" value="#055f40" />
</widget>
